    <link rel="icon" href="{% static 'images/logo.svg' %}">
</head>
<body>
<!--顶部导航条-->
{% block navbar %}
    <nav class="navbar navbar-dark sticky-top bg-dark flex-md-wrap p-0 shadow">
        <a class="navbar-brand col-md-3 col-lg-2 mr-0 px-3" href="/">
            <img src="{% static 'images/logo.svg' %}" width="30" height="30" alt="" loading="lazy">
            Automation</a>
        <a class="nav-item nav-link ml-auto text-white py-auto" href="/admin/">
